decl Bool
  dat True
  dat False

&&(True, True) = True
&&(:Bool, :Bool) = False

||(False, False) = False
||(:Bool, :Bool) = True

¬(True) = False
¬(False) = True
¬(f: t → Bool) = f ▹ (¬)

==(a, a) = True
==(a, b) = False

!=(a, b) = ¬(a == b)
