{
  "name": "elyon",
  "displayName": "Elyon",
  "description": "Elyon Language Support",
  "version": "0.0.1",
  "repository": "https://github.com/EidosX/Elyon",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "dev-install": "cp -R . ~/.vscode/extensions/elyon.elyon-0.0.1"
  },
  "contributes": {
    "languages": [
      {
        "id": "elyon",
        "aliases": [
          "Elyon",
          "elyon",
          "ely"
        ],
        "extensions": [
          ".ely"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "elyon",
        "scopeName": "source.elyon",
        "path": "./syntaxes/elyon.tmLanguage.json"
      }
    ]
  }
}
